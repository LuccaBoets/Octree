<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GIU_Median_Cut"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" x:Class="GIU_Median_Cut.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="canvas">
        <Button Content="Button" Click="openDialoge" Margin="10,15,699,380" RenderTransformOrigin="0.5,0.5">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="1.587"/>
                    <RotateTransform/>
                    <TranslateTransform X="0.499"/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Grid Name="gridColor" Height="100" Background="Aqua" Margin="0,334,0,0"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="252" VerticalAlignment="Top" Width="804" Margin="0,83,0,0"/>
    </Grid>
</Window>
